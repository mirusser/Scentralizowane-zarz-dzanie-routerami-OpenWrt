@model Dictionary<string, string>

@Styles.Render("~/bundles/jquery")
<script src="http://listjs.com/assets/javascripts/list.min.js"></script>

@{
    ViewBag.Title = "SendUciShow";
}

<h2>SendUciShow</h2>
<hr>
<div id="configurations">
    <table class="table table-striped table-bordered">
        <thead class="thead-inverse">
            <tr>
                <th class="sort" data-sort="key">Key</th>
                <th class="sort" data-sort="value">Value</th>
                <th colspan="2">
                    <input type="text" class="search" placeholder="Search configuration" />
                </th>
            </tr>
        </thead>
        <tbody class="list">
            @foreach (var item in Model)
            {
                <tr>
                    <td class="key">@item.Key</td>
                    <td class="value">@item.Value</td>
                </tr>
            }
        </tbody>
    </table>
</div>

<script>
    var options = {
        valueNames: ['key', 'value']
    };

    // Init list
    var configuration = new List('configurations', options);
</script>
